{
  "name": "react-auto-fill-box",
  "version": "0.1.3",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "keywords": [
    "react", "reactjs", "autofill", "autocomplete", "input", "field", "component", "form", "ui", "library", "javascript", "js"
  ],
  "description": "A simple Autofill/Autocomplete component for ReactJS.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sai-Raveendra-Kandregula/react-autofill-box.git"
  },
  "author": "Sai Raveendra Kandregula",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Sai-Raveendra-Kandregula/react-autofill-box/issues"
  },
  "homepage": "https://github.com/Sai-Raveendra-Kandregula/react-autofill-box#readme",
  "options": {
    "mocha": "--require scripts/mocha_runner src/**/__tests__/**/*.js"
  },
  "presets": ["env"],
  "scripts": {
    "prepublish": "babel src --ignore __tests__ --out-dir ./dist",
    "lint": "eslint ./src",
    "lintfix": "eslint ./src --fix",
    "testonly": "mocha $npm_package_options_mocha",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js"
  },
  "devDependencies": {
    "babel-cli": "^6.6.4",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-polyfill": "^6.7.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^2.7.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^4.2.3",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "nodemon": "^1.9.1",
    "sinon": "^1.17.3"
  },
  "peerDependencies": {
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1"
  }
}